{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "methods": ["GET"],
      "dest": "app.py"
    },
    {
      "src": "/join",
      "methods": ["GET"],
      "dest": "app.py"
    },
    {
      "src": "/status",
      "methods": ["GET"],
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  }
}
